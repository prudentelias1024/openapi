{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
    }, 
    { "src": "/todos", "methods": ["POST","PUT", "GET", "DELETE"], "dest": "./todoAPI.js" },
        
    { "src": "/products", "methods": ["POST","PUT", "GET", "DELETE"], "dest": "./productsAPI.js" },
        
    { "src": "/users", "methods": ["POST","PUT", "GET", "DELETE"], "dest": "./usersAPI.js" }
        
    
    ]
}
